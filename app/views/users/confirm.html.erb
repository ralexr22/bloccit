<h2>Sign Up Confirmation</h2>
  <h4>Are these values correct?</h4>
    <div class="row">
      <div class="col-md-8">
       <h5>Email: <%= @user.email%></h5>
       <h5>Name: <%= @user.name%></h5>
      </div>
      <div class="row">
       <div class="col-md-4">
        <%= button_to "Yes", {controller: "users", action: "create", params: params}, class: 'btn btn-success' %>
       </div>
       <div class="col-md-4">
        <%= button_to "No", {controller: "users", action: "new", params: params}, class: 'btn btn' %>
       </div>
     </div>
   </div>
